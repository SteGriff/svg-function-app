{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "index": {
      "matchCondition": {
        "route": "/"
      },
      "backendUri": "https://svgtopng.azurewebsites.net/api/Home"
    },
    "acme-challenge": {
      "matchCondition": {
        "route": "/.well-known/acme-challenge/{*rest}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/LetsEncrypt/{rest}"
    }
  }
}